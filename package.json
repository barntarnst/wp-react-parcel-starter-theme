{
  "name": "wp-react-parcel-starter-theme",
  "version": "1.0.0",
  "description": "Wordpress starter theme with React and Parcel",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel ./src/index.html",
    "build": "parcel build ./src/js/index.js --public-url /wp-content/themes/masterhuset/dist",
    "watch": "parcel watch ./src/js/index.js --public-url /wp-content/themes/masterhuset/dist",
    "pretti": "prettier --single-quote --no-semi --write ./src/js/**/**/*"
  },
  "keywords": [
    "parcel",
    "react",
    "wordpress"
  ],
  "author": "Accomplice AB",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.2",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "sass": "^1.14.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": ""
    }
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "google-map-react": "^1.0.9",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  }
}
